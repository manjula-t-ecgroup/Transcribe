    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-avatar": "^3.0.2",
    "react-avatar-edit": "^0.6.0",
    "react-bootstrap": "^0.32.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-hotkeys": "^1.1.4",
    "react-ionicons": "^2.1.6",
    "react-localization": "^1.0.10",
